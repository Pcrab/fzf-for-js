{
  "name": "fzf",
  "version": "0.0.13",
  "license": "SEE LICENSE IN license.txt",
  "description": "Do fuzzy matching using fzf algorithm in JavaScript",
  "keywords": [
    "fzf",
    "fuzzy-string-match",
    "fuzzy-search"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:modules && npm run build:types",
    "build:modules": "tsc && vite build",
    "build:types": "tsc -p tsconfig-to-build-types.json --outDir dist",
    "test": "jest src"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/fzf.umd.js",
  "module": "./dist/fzf.es.js",
  "exports": {
    ".": {
      "import": "./dist/fzf.es.js",
      "require": "./dist/fzf.umd.js"
    }
  },
  "types": "./dist/main.d.ts",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/jest-expect-message": "^1.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.6",
    "history": "^5.0.0",
    "jest": "^27.0.6",
    "jest-expect-message": "^1.0.2",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "vite": "^2.3.8"
  }
}
